@model List<UserEntry>
@{

    ViewBag.Title = "Dashboard";
    
}

<div class="breadcrumbs" id="breadcrumbs">
    <ul class="breadcrumb">
        <li>
            <i class="ace-icon icon-home home-icon"></i>
            <a href="#">Setup</a>
        </li>
        <li class="active">User List</li>
    </ul>
</div>

<section id="container">
    <div class="all-margin">
        <div class="cust-row clear-lr-margin">
            <div class="btn-group">
                <a href="~/Users/Create" class="btn btn-default btn-bgcolor powertip" title="Add"><i class="icon-plus"></i></a>
                <a href="#" class="btn btn-default btn-bgcolor powertip" title="Edit"><i class="icon-edit"></i></a>
                <a href="#" class="btn btn-default btn-bgcolor powertip" title="Delete"><i class="icon-remove"></i></a>
            </div>
        </div>
        <div class="row cust-row">
            <div class="col-xs-12">
                <!-- table -->
                <table class="table celled table-striped  table-hover">
                    <thead>
                        <tr>
                            <th class="center">
                                <input type="checkbox" />
                            </th>
                            <th>User Name</th>
                            <th>First Name</th>
                            <th>Last Name</th>
                            <th>Email</th>
                            <th>Created By</th>
                            <th>Created Time</th>
                            <th class="btn-item2"></th>
                        </tr>
                    </thead>

                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td class="center">
                                    <input type="checkbox" value="@item.UserId" />
                                </td>
                                <td>@item.UserName</td>
                                <td>@item.FirstName</td>
                                <td>@item.LastName</td>
                                <td>@item.Email</td>
                                <td>@item.CreatedBy</td>
                                <td>@item.CreatedTime</td>
                                <td>
                                    <div class="btn-group">
                                        <a href="#" class="btn btn-info btn-sm"><i class="icon-pencil"></i></a>
                                        <a href="#" class="btn btn-danger btn-sm"><i class="icon-trash"></i></a>
                                    </div>
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
                <!-- end table -->

            </div>
        </div>
    </div>
</section>
